{
  "name": "spooner-web/just_content",
  "type": "typo3-cms-extension",
  "description": "Assign content and pages to backend users to prevent outdated content.",
  "homepage": "https://typo3.org",
  "license": [
    "GPL-2.0+"
  ],
  "keywords": [
    "TYPO3",
    "extension",
    "content",
    "plan",
    "assignment",
    "outdated"
  ],
  "authors": [
    {
      "name": "Thomas LÃ¶ffler",
      "email": "loeffler@spooner-web.de",
      "homepage": "https://spooner-web.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "typo3/cms-core": "^8"
  },
  "autoload": {
    "psr-4": {
      "SpoonerWeb\\JustContent\\": "Classes/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
